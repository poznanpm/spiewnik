<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 3.0.2" modifiedIn="OpenLP 3.0.2" modifiedDate="2024-02-13T20:39:07">
  <properties>
    <titles>
      <title>Ja potrzebuję Ciebie</title>
    </titles>
    <authors>
      <author>PM</author>
    </authors>
    <songbooks>
      <songbook name="Śpiewnik PM"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Przyjdź Duchu Święty, przyjdź! Niepojęty<br/>Przyjdź! Objaw serce swoje mi.</lines>
    </verse>
    <verse name="c1">
      <lines>Refren:<br/>Ja potrzebuję Ciebie<br/>i miłości Twej.<br/>Ja potrzebuję Ciebie,<br/>Duchu Święty wiej!</lines>
    </verse>
  </lyrics>
</song>
