    <titles>
      <title>Ześlij deszcz</title>
      <title>czuję deszcz miłości twej</title>
    </titles>
    <authors>
      <author>PM</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="c1">
      <lines><br/>Ześlij deszcz,<br/>ześlij deszcz otwórzcie się bramy nieba.<br/><br/></lines>
    </verse>
    <verse name="v2">
      <lines><br/>Przelewają się przeze mnie rzeki Twojej łaski.<br/>Łaska po łasce fala za falą. x2<br/>Łaska po łasce.</lines>
    </verse>
  </lyrics>
